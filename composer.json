        "symfony/messenger": "^5.0",
        "symfony/options-resolver": "^5.0",
        "symfony/polyfill-mbstring": "^1.12",
        "symfony/polyfill-php72": "^1.12",
        "symfony/polyfill-php73": "^1.12",
        "symfony/postmark-mailer": "^5.0",
        "symfony/sendgrid-mailer": "^5.0",
        "symfony/string": "^5.0",
